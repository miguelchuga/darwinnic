            <field name="model">account.journal</field>
            <field name="inherit_id" ref="account.view_account_journal_form"/>
            <field name="arch" type="xml">
                <xpath expr="//form//sheet/group/group[1]" position="inside">

                    <field name="infilefel_type"/>
