                    <field name="infilefel_comercial_name"/>
                    <field name="infilefel_establishment_street"/>
                    <field name="infilefel_phone_number"/>
                    <field name="ws_url_pdf"/>
                    <!-- YA NO SE VAN A UTILIZAR
                    <field name="infilefel_previous_authorization"/>
                    <field name="infilefel_previous_serial"/>
-->

                </xpath>
            </field>
        </record>
